<Button>:
    canvas.before:
		RoundedRectangle:
			size: self.size
			pos: self.pos

<LoadingScreen>:
    name: 'loading'

<ListScreen>:
    name: 'list'
    FloatLayout:
        Image:
            source: 'data/home.gif'  # Replace with the path to your GIF file
            anim_delay: 1 / 30  # Adjust the delay according to your GIF's frame rate
            allow_stretch: True
            keep_ratio: False
            pos: self.pos
            size: self.size

    BoxLayout:
        orientation: 'vertical'
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        size_hint: 0.1, 0.1

        TransparentButton:
            size_hint: 2, 2
            pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            on_release: root.manager.start_main()

            Image:
                source: 'data/btn/start.png'
                allow_stretch: True
                keep_ratio: False
                size: self.parent.size
                pos: self.parent.pos

    BoxLayout:
        orientation: 'vertical'
        pos_hint: {'center_x': .87, 'center_y': .93}
        size_hint: .1, .1

        TransparentButton:
            size_hint: None, 0.5
            pos_hint: {'center_x': .87, 'center_y': .93}
            on_release: root.start_instructions()

            Image:
                source: 'data/btn/instruction.png'
                allow_stretch: True
                keep_ratio: False
                size: self.parent.size
                pos: self.parent.pos

    BoxLayout:
